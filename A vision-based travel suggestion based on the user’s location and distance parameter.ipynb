    "import requests\n",
    "import pandas as pd\n",
    "from pandas import option_context\n",
    "from collections import defaultdict\n",
    "\n",
    "url = 'https://api.geoapify.com/v2/places'\n",
    "\n",
    "category = input(\"Enter a category from the list available : \")\n",
    "params = dict(categories = category, filter='circle:%s,%s,40000' % (float(ls[1]), float(ls[0])), limit= 20, apiKey="YOUR API KEY")\n",
    "\n",
    "resp = requests.get(url=url, params=params)\n",
    "data = resp.json()\n",
    "if ('error' in data):\n",
    "    print(\"Enter a Valid Category\")\n",
    "else:\n",
    "    n = []\n",
    "    c = []\n",
    "    s = []\n",
    "    lon = []\n",
    "    lat = []\n",
    "    add = []\n",
    "    for i in data['features']:\n",
    "        if ('name' not in i['properties']):\n",
    "            n.append(\"Not Available\")\n",
    "        else:\n",
    "            n.append(i['properties']['name'])\n",
    "\n",
